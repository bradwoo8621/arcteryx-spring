<?xml version="1.0" encoding="UTF-8"?>

<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"
	default-lazy-init="true">

	<bean class="com.github.nnest.arcteryx.spring.SpringEnterpriseAware" />
	<bean class="com.github.nnest.arcteryx.Enterprise" />
	<bean
		id="App1"
		class="com.github.nnest.arcteryx.Application">
		<constructor-arg
			type="java.lang.String"
			value="App1" />
	</bean>
	<bean
		id="App2"
		class="com.github.nnest.arcteryx.Application">
		<constructor-arg
			type="java.lang.String"
			value="App2" />
	</bean>

	<bean
		id="Comp11"
		class="com.github.nnest.arcteryx.Component"
		p:container-ref="App1">
		<constructor-arg
			type="java.lang.String"
			value="Comp11" />
	</bean>
	<bean
		class="com.github.nnest.arcteryx.Component"
		p:container-ref="App1">
		<constructor-arg
			type="java.lang.String"
			value="Comp12" />
	</bean>

	<bean
		class="com.github.nnest.arcteryx.Component"
		p:container-ref="App2">
		<constructor-arg
			type="java.lang.String"
			value="Comp21" />
	</bean>
	<bean
		class="com.github.nnest.arcteryx.Component"
		p:container-ref="App2">
		<constructor-arg
			type="java.lang.String"
			value="Comp22" />
	</bean>

	<bean
		class="com.github.nnest.arcteryx.spring.CustomResource"
		p:container-ref="Comp11">
		<constructor-arg
			type="java.lang.String"
			value="Res111" />
	</bean>
</beans>