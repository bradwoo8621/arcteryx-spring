<?xml version="1.0" encoding="UTF-8"?>

<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"
	default-lazy-init="true">
	<bean
		id="CustomApp"
		class="com.github.nest.arcteryx.spring.bootstrap.CustomApplication" />
	<bean
		id="CustomComp"
		class="com.github.nest.arcteryx.spring.bootstrap.CustomComponent" />
	<bean
		class="com.github.nest.arcteryx.spring.bootstrap.CustomResource"
		p:container-ref="CustomComp">
		<constructor-arg
			type="java.lang.String"
			value="CustomResA" />
	</bean>
</beans>